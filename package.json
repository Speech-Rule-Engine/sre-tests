{
  "name": "sre-tests",
  "version": "0.9.0",
  "description": "Tests for SRE.",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "prepare-webpack": "npx rimraf js; npx rimraf dist; ./node_modules/.bin/tsc; ./node_modules/.bin/webpack",
    "prepare": "node tests.config.js; npm run prepare-webpack",
    "test": "npm run test-files",
    "test-all": "npm run test-files; npm run test-output",
    "test-files": "npx jest --testPathPattern js --testPathIgnorePatterns js/output",
    "test-output": "npx jest --testPathPattern js/output",
    "clean": "rm -rf dist js ts/output ts/jest coverage"
  },
  "maintainers": [
    "Volker Sorge <Volker.Sorge@gmail.com> (http://www.cs.bham.ac.uk/~vxs)"
  ],
  "bugs": {
    "email": "volker.sorge@gmail.com",
    "url": "https://github.com/speech-rule-engine/sre-tests/issues"
  },
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/speech-rule-engine/sre-tests"
  },
  "keywords": [
    "screen-reader",
    "xml",
    "svg",
    "a11y"
  ],
  "homepage": "https://github.com/speech-rule-engine/sre-tests",
  "dependencies": {
    "wicked-good-xpath": "^1.3.0",
    "xmldom-sre": "^0.1.31"
  },
  "devDependencies": {
    "@types/node": "^14.11.5",
    "@types/jest": "^26.0.14",
    "terser-webpack-plugin": "^4.2.2",
    "ts-loader": "^8.0.4",
    "tslint": "^6.1.3",
    "tslint-jsdoc-rules": "^0.2.0",
    "tslint-unix-formatter": "^0.2.0",
    "typescript": "^4.0.3",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "@babel/preset-env": "^7.11.5",
    "babel-jest": "^26.5.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "jest": "^26.5.2"
  },
  "files": [
    "LICENSE",
    "README.md",
    "lib/"
  ],
  "main": "dist/sretests.js"
}
