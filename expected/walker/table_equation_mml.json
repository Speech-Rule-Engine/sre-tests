{
  "information": "Table walker with Node generator on Equation Mml expression.",
  "walker": "Table",
  "generator": "Node",
  "base": "input/base/walker_test.json",
  "factory": "walker",
  "renderer": "NativeMML",
  "expression": "Equation_Mml",
  "tests": {
    "Step_0": {
      "input": null,
      "expected": "StartLayout 1st Row 1st Column a 2nd Column equals 3rd Column b 2nd Row 1st Column c 2nd Column equals 3rd Column d EndLayout"},
    "Step_1": {
      "input": "X",
      "expected": "table with 2 rows and 3 columns"},
    "Step_2": {
      "input": "DOWN",
      "expected": "1st Row 1st Column a 2nd Column equals 3rd Column b"},
    "Step_3": {
      "input": "RIGHT",
      "expected": "2nd Row 1st Column c 2nd Column equals 3rd Column d"},
    "Step_4": {
      "input": "RIGHT",
      "expected": "2nd Row 1st Column c 2nd Column equals 3rd Column d"},
    "Step_5": {
      "input": "SPACE",
      "expected": "Level 1 2nd Row"},
    "Step_6": {
      "input": "X",
      "expected": "2nd Row in table with 3 columns"},
    "Step_7": {
      "input": "DOWN",
      "expected": "1st Column c"},
    "Step_8": {
      "modifier": true,
      "input": "UP",
      "expected": "1st Column a"},
    "Step_9": {
      "input": "SPACE",
      "expected": "Level 2 1st Row 1st Column"},
    "Step_10": {
      "modifier": true,
      "input": "DOWN",
      "expected": "1st Column c"},
    "Step_11": {
      "input": "SPACE",
      "expected": "Level 2 2nd Row 1st Column"},
    "Step_12": {
      "input": "X",
      "expected": "1st Column in table"},
    "Step_13": {
      "input": "DOWN",
      "expected": "c"},
    "Step_14": {
      "input": "UP",
      "expected": "1st Column c"},
    "Step_15": {
      "input": "RIGHT",
      "expected": "2nd Column equals"},
    "Step_16": {
      "input": "SPACE",
      "expected": "Level 2 2nd Row 2nd Column"},
    "Step_17": {
      "input": "X",
      "expected": "2nd Column in table"},
    "Step_18": {
      "input": "UP",
      "expected": "2nd Row 1st Column c 2nd Column equals 3rd Column d"},
    "Step_19": {
      "input": "LEFT",
      "expected": "1st Row 1st Column a 2nd Column equals 3rd Column b"},
    "Step_20": {
      "input": "HOME",
      "expected": "StartLayout 1st Row 1st Column a 2nd Column equals 3rd Column b 2nd Row 1st Column c 2nd Column equals 3rd Column d EndLayout"},
    "Step_21": {
      "input": "DOWN",
      "expected": "1st Row 1st Column a 2nd Column equals 3rd Column b"},
    "Step_22": {
      "input": "X",
      "expected": "1st Row in table with 3 columns"},
    "Step_23": {
      "input": "2",
      "expected": "1st Row 1st Column a 2nd Column equals 3rd Column b"},
    "Step_24": {
      "input": "2",
      "expected": "2nd Column equals"},
    "Step_25": {
      "input": "SPACE",
      "expected": "Level 2 2nd Row 2nd Column"},
    "Step_26": {
      "input": "2",
      "expected": "2nd Column equals"},
    "Step_27": {
      "input": "3",
      "expected": "3rd Column d"},
    "Step_28": {
      "input": "SPACE",
      "expected": "Level 2 2nd Row 3rd Column"},
    "Step_29": {
      "input": "UP",
      "expected": "2nd Row 1st Column c 2nd Column equals 3rd Column d"}
  }
}
